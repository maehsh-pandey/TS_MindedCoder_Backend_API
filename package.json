  "main": "index.js",
  "scripts": {
    "test": "node ./test.js",
    "dev": "dotenvx run -f .env.development -- nodemon ./src/server.ts",
    "start": "cross-env NODE_ENV=production nodemon ./dist/server.js",
    "start:env:encrypt": "dotenvx encrypt -f .env.development",
